<h1> Student's Page</h1>

<%= link_to 'Add A New Student', new_student_path %>

<div>

<% @students.each do |student| %>
<div>
<%= student.first_name %>
<%= student.last_name %>
<%= link_to 'Edit Student', edit_student_path(student) %> |
<%= link_to 'View Student Information', student_path(student) %> |
<%= link_to 'Delete Student', student_path(student), method: :delete %>
</div>

<% end %>
</div>
